<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CompanyInfo">
   <children>
      <Label layoutX="134.0" layoutY="38.0" text="Просмотр информации о компании">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <GridPane layoutX="100.0" layoutY="176.0" prefHeight="48.0" prefWidth="401.0">
         <children>
            <Label prefHeight="21.0" prefWidth="224.0" text="Введите название компании:" />
            <TextField fx:id="companyName" onAction="#NextButton" promptText="Компания" GridPane.columnIndex="1" />
         </children>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="248.0" minWidth="10.0" prefWidth="205.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="196.0" minWidth="10.0" prefWidth="196.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
      </GridPane>
      <Button layoutX="500.0" layoutY="340.0" mnemonicParsing="false" onAction="#NextButton" text="Далее" />
      <Button layoutX="35.0" layoutY="340.0" mnemonicParsing="false" onAction="#BackButton" text="Назад" />
   </children>
</AnchorPane>
